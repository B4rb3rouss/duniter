    "moment": "2.18.1",
    "node-pre-gyp": "0.6.34",
    "optimist": "0.6.1",
    "q": "1.5.0",
    "q-io": "1.13.2",
    "querablep": "^0.1.0",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "sha1": "1.1.1",
